cat <<EOF
<?xml version="1.0"?>
<gtester>
 <info>
  <package>qemu</package>
  <version>0.0</version>
  <revision>rev</revision>
 </info>
EOF

sed \
  -e '/<?xml/d' \
  -e '/^<gtester>$/d' \
  -e '/<info>/,/<\/info>/d' \
  -e '$b' \
  -e '/^<\/gtester>$/d' "$@"
